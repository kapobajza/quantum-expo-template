    "axios": "1.10.0",
    "babel-plugin-react-compiler": "19.1.0-rc.2",
    "dayjs": "1.11.13",
    "drizzle-orm": "0.44.2",
    "eslint-plugin-react-compiler": "19.1.0-rc.2",
    "expo": "54.0.25",
    "expo-constants": "18.0.10",
    "expo-crypto": "15.0.7",
    "expo-dev-client": "6.0.18",
    "expo-font": "14.0.9",
