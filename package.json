    "eslint-plugin-import-x": "4.12.2",
    "eslint-plugin-prettier": "5.4.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "jsdom": "26.1.0",
    "lefthook": "1.11.12",
    "nodemon": "3.1.10",
    "prettier": "3.5.3",
    "react-test-renderer": "19.0.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.35.1",
    "vite-tsconfig-paths": "5.1.4",
