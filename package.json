{
  "name": "quantum-expo-template",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "prebuild": "expo prebuild --clean",
    "android": "expo run:android",
    "ios": "expo run ios",
    "android:clean": "pnpm run prebuild && pnpm run android",
    "ios:clean": "pnpm run prebuild && pnpm run ios",
    "web": "expo start --web",
    "test:watch": "vitest",
    "test": "vitest run",
    "lint": "eslint . --max-warnings 0",
    "typecheck": "tsc",
    "migrate": "drizzle-kit generate",
    "generate_translation_types": "nodemon --watch src/locale/resources --watch translation-schema.json --ext json scripts/generate_translation_types.js",
    "generate_svg_icons": "nodemon --watch assets/icons --watch svgr.config.js --ext svg --exec \"npx @svgr/cli assets/icons\"",
    "commit": "commit"
  },
  "dependencies": {
    "@hookform/resolvers": "5.0.1",
    "@react-navigation/bottom-tabs": "7.3.10",
    "@react-navigation/elements": "2.3.8",
