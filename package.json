{
  "name": "quantum-expo-template",
  "main": "./src/App.tsx",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "prebuild": "cross-env EXPO_NO_GIT_STATUS=1 expo prebuild --clean",
    "android": "expo run:android",
    "ios": "expo run ios",
    "android:clean": "pnpm run prebuild -p android && pnpm run android",
    "ios:clean": "pnpm run prebuild -p ios && pnpm run ios",
    "web": "expo start --web",
    "test:watch": "vitest",
    "test": "vitest run",
    "lint": "eslint . --max-warnings 0",
    "typecheck": "tsc",
    "migrate": "drizzle-kit generate",
    "generate_translation_types": "nodemon --watch src/locale/resources --watch src/locale/translation-schema.json --ext json scripts/generate_translation_types.js",
    "gen:icons": "npx @svgr/cli assets/icons",
    "gen:icons:watch": "nodemon --watch assets/icons --watch svgr.config.js --ext svg --exec \"pnpm run gen:icons\"",
    "rename_to_dot_files": "node scripts/rename_to_dot_files.js",
    "storybook-generate": "sb-rn-get-stories",
    "storybook:start": "cross-env EXPO_PUBLIC_STORYBOOK_ENABLED='true' pnpm run start",
    "storybook:ios": "cross-env EXPO_PUBLIC_STORYBOOK_ENABLED='true' pnpm run ios",
    "storybook:android": "cross-env EXPO_PUBLIC_STORYBOOK_ENABLED='true' pnpm run android"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "5.2.6",
